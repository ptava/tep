FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outlet
    {
        type            fixedValue;
        value           $internalField;
    }

    inlet
    {
        type            zeroGradient;
    }


    ground
    {
       
        type            zeroGradient;
    }

    top
    {
       
        type            zeroGradient;
    }

    lateral1
    {
        type            symmetryPlane;
    }

    lateral2
    {
        type            symmetryPlane;
    }
}


